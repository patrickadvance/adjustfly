{
    "name": "patrixsmart/adjustfly",
    "description": "Record Laravel model adjustments during updating event.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Patrick Duruamadi",
            "email": "patrickadvance@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "laravel/framework": "^8.0|^9.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Patrixsmart\\Adjustfly\\AdjustflyServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Patrixsmart\\Adjustfly\\": "src/",
            "Patrixsmart\\Adjustfly\\Database\\Factories\\": "database/factories/",
            "Patrixsmart\\Adjustfly\\Database\\Seeders\\": "database/seeders/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
